{                                                                                                                                                                                                                                                              
	"name": "monstersvsfs",                                                                                                                                                                                                                                       
	"version": "0.0.1",                                                                                                                                                                                                                                           
	"private": true,                                                                                                                                                                                                                                              
	"description": "Monster List VS Code Freestyle",                                                                                                                                                                                                              
	"keywords": [                                                                                                                                                                                                                                                 
		"ui5",                                                                                                                                                                                                                                                       
		"openui5",                                                                                                                                                                                                                                                   
		"sapui5"                                                                                                                                                                                                                                                     
	],                                                                                                                                                                                                                                                            
	"main": "webapp/index.html",                                                                                                                                                                                                                                  
	"scripts": {                                                                                                                                                                                                                                                  
		"start": "fiori run --open 'index.html?sap-client=249'",                                                                                                                                                                                                     
		"start-local": "fiori run --config ./ui5-local.yaml --open 'index.html?sap-client=249'",                                                                                                                                                                     
		"start-noflp": "fiori run --open 'index.html?sap-client=249'",                                                                                                                                                                                               
		"build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",                                                                                                                                                          
		"deploy": "fiori verify",                                                                                                                                                                                                                                    
		"deploy": "fiori add deploy-config",                                                                                                                                                                                                                         
		"unit-tests": "fiori run --open test/unit/unitTests.qunit.html",                                                                                                                                                                                             
		"int-tests": "fiori run --open test/integration/opaTests.qunit.html"                                                                                                                                                                                         
	},                                                                                                                                                                                                                                                            
	"devDependencies": {                                                                                                                                                                                                                                          
		"@ui5/cli": "^2.11.1",                                                                                                                                                                                                                                       
		"@ui5/fs": "^2.0.6",                                                                                                                                                                                                                                         
		"@ui5/logger": "^2.0.1",                                                                                                                                                                                                                                     
		"@sap/ux-ui5-tooling": "1",                                                                                                                                                                                                                                  
		"rimraf": "3.0.2"                                                                                                                                                                                                                                            
	},                                                                                                                                                                                                                                                            
	"ui5": {                                                                                                                                                                                                                                                      
		"dependencies": [                                                                                                                                                                                                                                            
			"@sap/ux-ui5-tooling"                                                                                                                                                                                                                                       
		]                                                                                                                                                                                                                                                            
	}                                                                                                                                                                                                                                                             
}                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               