    _this.options = transformOptions(options);                                                                                                                                                                                                                 
    _this.services = {};                                                                                                                                                                                                                                       
    _this.logger = baseLogger;                                                                                                                                                                                                                                 
    _this.modules = {                                                                                                                                                                                                                                          
      external: []                                                                                                                                                                                                                                             
    };                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
