      var newLngs = lngs.filter(function (lng) {                                                                                                                                                                                                               
        return preloaded.indexOf(lng) < 0;                                                                                                                                                                                                                     
      }); // Exit early if all given languages are already preloaded                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
