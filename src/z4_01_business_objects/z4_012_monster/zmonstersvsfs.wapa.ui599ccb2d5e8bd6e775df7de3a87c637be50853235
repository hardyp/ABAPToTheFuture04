        s.resourceStore = this.store;                                                                                                                                                                                                                          
        s.languageUtils = lu;                                                                                                                                                                                                                                  
        s.pluralResolver = new PluralResolver(lu, {                                                                                                                                                                                                            
          prepend: this.options.pluralSeparator,                                                                                                                                                                                                               
