function isPrimitive(arg) {                                                                                                                                                                                                                                    
  return arg === null ||                                                                                                                                                                                                                                       
         typeof arg === 'boolean' ||                                                                                                                                                                                                                           
         typeof arg === 'number' ||                                                                                                                                                                                                                            
         typeof arg === 'string' ||                                                                                                                                                                                                                            
         typeof arg === 'symbol' ||  // ES6 symbol                                                                                                                                                                                                             
         typeof arg === 'undefined';                                                                                                                                                                                                                           
}                                                                                                                                                                                                                                                              
exports.isPrimitive = isPrimitive;                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
exports.isBuffer = Buffer.isBuffer;                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
function objectToString(o) {                                                                                                                                                                                                                                   
  return Object.prototype.toString.call(o);                                                                                                                                                                                                                    
}                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
