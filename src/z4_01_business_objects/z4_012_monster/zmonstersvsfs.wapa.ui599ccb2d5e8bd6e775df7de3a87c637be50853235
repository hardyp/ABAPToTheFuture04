/***/ ((module, __unused_webpack_exports, __webpack_require__) => {                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
/**                                                                                                                                                                                                                                                            
 * For Node.js, simply re-export the core `util.deprecate` function.                                                                                                                                                                                           
 */                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
module.exports = __webpack_require__(31669).deprecate;                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
