/***/ 22156:                                                                                                                                                                                                                                                   
/***/ ((module) => {                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
"use strict";                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
// this exists so we can replace it during testing                                                                                                                                                                                                             
module.exports = process                                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
