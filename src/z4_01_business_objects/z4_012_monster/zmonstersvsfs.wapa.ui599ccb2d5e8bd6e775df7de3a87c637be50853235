/**                                                                                                                                                                                                                                                            
 * Define a lazy property.                                                                                                                                                                                                                                     
 */                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
function lazyProperty (obj, prop, getter) {                                                                                                                                                                                                                    
  function get () {                                                                                                                                                                                                                                            
    var val = getter()                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
    Object.defineProperty(obj, prop, {                                                                                                                                                                                                                         
      configurable: true,                                                                                                                                                                                                                                      
      enumerable: true,                                                                                                                                                                                                                                        
      value: val                                                                                                                                                                                                                                               
    })                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
    return val                                                                                                                                                                                                                                                 
  }                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
  Object.defineProperty(obj, prop, {                                                                                                                                                                                                                           
    configurable: true,                                                                                                                                                                                                                                        
    enumerable: true,                                                                                                                                                                                                                                          
    get: get                                                                                                                                                                                                                                                   
  })                                                                                                                                                                                                                                                           
}                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
/**                                                                                                                                                                                                                                                            
 * Call toString() on the obj                                                                                                                                                                                                                                  
 */                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
function toString (obj) {                                                                                                                                                                                                                                      
  return obj.toString()                                                                                                                                                                                                                                        
}                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
