    buf.fill(0)                                                                                                                                                                                                                                                
  }                                                                                                                                                                                                                                                            
  return buf                                                                                                                                                                                                                                                   
}                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
