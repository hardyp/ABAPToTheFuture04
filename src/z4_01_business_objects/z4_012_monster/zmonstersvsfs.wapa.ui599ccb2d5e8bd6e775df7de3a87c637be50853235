  // implementation from standard node.js 'util' module                                                                                                                                                                                                        
  module.exports = function inherits(ctor, superCtor) {                                                                                                                                                                                                        
    if (superCtor) {                                                                                                                                                                                                                                           
      ctor.super_ = superCtor                                                                                                                                                                                                                                  
      ctor.prototype = Object.create(superCtor.prototype, {                                                                                                                                                                                                    
